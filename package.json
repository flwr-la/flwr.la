{
  "name": "flwr.la",
  "version": "0.1.0",
  "description": "Dynamic Linked LLM Module System",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:prod": "tsc && node scripts/optimize.js",
    "docker:build": "docker build -t flwr.la:latest .",
    "docker:run": "docker-compose up -d",
    "migrate": "node scripts/migrate.js",
    "seed:db": "node scripts/seed-db.js",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "docs": "typedoc src --out docs/api",
    "prepare": "husky install"
  },
  "keywords": [
    "llm",
    "ai",
    "modular",
    "ephemeral"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "openai": "^4.0.0",
    "uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "winston": "^3.8.0",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "rate-limiter-flexible": "^2.4.1",
    "joi": "^17.9.0",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "typeorm": "^0.3.16",
    "redis": "^4.6.0",
    "bull": "^4.10.0",
    "socket.io": "^4.6.0",
    "@sentry/node": "^7.0.0",
    "prom-client": "^14.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/bcrypt": "^5.0.0",
    "prettier": "^2.8.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "supertest": "^6.3.0",
    "@types/supertest": "^2.0.12",
    "webpack-bundle-analyzer": "^4.8.0",
    "typedoc": "^0.24.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}